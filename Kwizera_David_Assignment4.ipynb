      "cell_type": "code",
      "metadata": {
        "id": "NkccRtzsUuZU"
      },
      "source": [
        "# Shuffling the data\n",
        "df = df.reindex(np.random.permutation(df.index))\n",
        "\n",
        "# Splitting features from the label\n",
        "data = df['article'].values\n",
        "target = df['category'].values"
      ],
      "execution_count": null,
      "outputs": []
